package datastruct;
/*栈数据结构
 * 作者：夜苍山
 */
public class Stack {
	private int[] S;
	private int top;
	public Stack(int capacity) {
		this.S=new int[capacity];
		this.top=-1;
	}
	public void push(int key) {
		S[++top]=key;
	}
	public int pop() {
		if(top==-1)
			return -1;
		return S[--top+1];
	}
	
	public static void main(String[] args) {
		Stack s=new Stack(2);
		s.push(2);
		s.push(4);
		s.push(6);
		System.out.print(s.pop());
		System.out.print(s.pop());
		System.out.print(s.pop());
		System.out.print(s.pop());
	}
}
