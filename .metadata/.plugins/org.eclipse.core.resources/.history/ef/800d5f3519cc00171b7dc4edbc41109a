package datastruct;

public class Queue {
	private int[] Q;
	private int head;
	private int tail;
	public Queue() {
		this.Q=new int[5];
		this.head=0;
		this.tail=0;
	}
	public void enQueue(int key) {
			Q[tail]=key;
			if(tail==Q.length-1)
				tail=0;
			else 
				tail=tail+1;
			if(head==tail)
				System.out.println("Queue full");
		}
	public int deQueue() {
		int key=Q[head];
		if(head==Q.length-1)
			head=0;
		else
			head=head+1;
		if(head==tail)
			System.out.println("Queue empty");
		return key;
	}
	public static void main(String[] args) {
		Queue q=new Queue();
		q.enQueue(3);
		q.enQueue(13);
		q.deQueue();
		q.enQueue(23);
		q.deQueue();
		System.out.print("get head:" + q.deQueue());
	
		/*
		q.enQueue(33);
		q.enQueue(43);
		//q.enQueue(53);
		q.deQueue();
		q.deQueue();
		q.deQueue();
		q.deQueue();
		q.deQueue();
		*/
		
		/*
		for(int i=1;i<10;i++)
			q.enQueue(i);
			*/
	}
}
